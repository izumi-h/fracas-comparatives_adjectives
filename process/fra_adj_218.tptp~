tff(th_type, type , th : $i > $int).
tff(clever_type, type , clever : $i * $int > $o).
tff(person_type, type , person : $i > $o).
tff(t1,axiom,![X2]: ((clever(X2,np(u,th(u))) <=> clever(X2,np(person,th(person)))))).
tff(t2,axiom,![D3:$int]: (![X4]: ((clever(X4,D3) <=> ![D2:$int]: (($lesseq(D2,D3) => clever(X4,D2))))))).
tff(t3,axiom,![X5]: (![X6]: ((?[D7:$int]: ((clever(X5,D7) & (~clever(X6,D7)))) => ![D8:$int]: ((clever(X6,D8) => clever(X5,D8))))))).
tff(t4,axiom,(clever(kim,np(person,th(person))) & person(kim))).
tff(h,conjecture,(~clever(kim,np(u,th(u))))).
